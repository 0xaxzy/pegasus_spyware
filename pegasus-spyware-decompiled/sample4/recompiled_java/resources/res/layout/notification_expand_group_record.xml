<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="horizontal" android:paddingLeft="9dp" android:paddingTop="5dp" android:paddingRight="5dp" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:minHeight="60dp">
    <ImageView android:id="@+id/app_icon" android:layout_width="36dp" android:layout_height="36dp" android:layout_marginTop="7dp"/>
    <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_weight="1">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="16sp" android:textColor="@color/dark" android:ellipsize="end" android:id="@+id/app_label" android:layout_width="0dp" android:layout_height="wrap_content" android:singleLine="true" android:layout_weight="1"/>
            <TextView android:textSize="12sp" android:textColor="@color/dark" android:ellipsize="end" android:id="@+id/notification_intecept_time" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="14sp" android:textColor="@color/grey" android:ellipsize="end" android:id="@+id/notification_content_text" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="6dp" android:singleLine="true" android:layout_weight="1"/>
            <TextView android:textSize="12sp" android:textColor="@color/dark" android:ellipsize="end" android:id="@+id/notification_intecept_permission" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

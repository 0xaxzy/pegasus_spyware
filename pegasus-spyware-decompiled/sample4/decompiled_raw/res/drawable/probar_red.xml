<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff474747" android:endColor="#ff474747" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff474747" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff9a1919" android:endColor="#ff9a1919" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff9a1919" />
            </shape>
        </clip>
    </item>
</layer-list>
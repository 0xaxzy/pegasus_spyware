<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="NavPage">
        <item name="android:gravity">0x1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">#eeeeee</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="NegativeButtonText">
        <item name="android:textSize">19dp</item>
        <item name="android:textColor">@drawable/home_page_button_healthcheckup_color_selector</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/signbtn_bg</item>
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:shadowDx">@null</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>
    <style name="BriefText">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#ff707070</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#a0ffffff</item>
        <item name="android:shadowDx">1.2</item>
        <item name="android:shadowDy">1.2</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:lineSpacingExtra">2dp</item>
    </style>
    <style name="MMLabelSplit">
        <item name="android:gravity">0x11</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">13dp</item>
    </style>
    <style name="Transparent">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>
    <style name="MMThemeLauncher">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowTitleSize">0dp</item>
    </style>
    <style name="trans_edit_text_style" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/trans_edit_text</item>
    </style>
    <style name="Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="DialogWallpaper" parent="@android:style/Theme.Wallpaper">
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="DialogText">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="DialogText_Title" parent="@style/DialogText">
        <item name="android:textSize">16dp</item>
        <item name="android:textStyle">0x1</item>
    </style>
    <style name="DialogListText">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="ButtonText">
        <item name="android:textSize">19dp</item>
        <item name="android:textColor">@color/black4</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/home_page_button_healthcheckup_selector</item>
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:shadowDx">@null</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>
</resources>

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:opti="http://schemas.android.com/apk/res/com.lenovo.performance" android:orientation="vertical" android:background="@drawable/listview_normal" android:layout_width="match_parent" android:layout_height="match_parent">
    <include android:id="@+id/mansms_framelayout" android:visibility="gone" layout="@layout/title"/>
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:gravity="center_vertical" android:id="@+id/bootstart_time" android:text="@string/wisdom_speed_description" style="@style/TextBackground"/>
        <RelativeLayout android:orientation="horizontal" android:id="@+id/rlayout1" android:layout_width="wrap_content" android:layout_height="60dp">
            <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:layout_gravity="center_vertical" android:id="@+id/text1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:text="@string/wisdom_speed" android:layout_alignParentLeft="true" android:layout_centerVertical="true"/>
            <com.lenovo.performancecenter.coreui.widget.CheckBoxPreference android:gravity="center_vertical" android:layout_gravity="center_vertical" android:id="@+id/btnLeemSwitch" android:layout_width="72dp" android:layout_height="34dp" android:layout_marginRight="10dp" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
